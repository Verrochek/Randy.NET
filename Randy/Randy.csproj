<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <PackageId>Randy.NET</PackageId>
        <Title>Randy.NET</Title>
        <Authors>Verrok</Authors>
        <Description>.NET Client for Random.org gRPC Api https://api.random.org/json-rpc/2</Description>
        <PackageProjectUrl>https://github.com/Verrok/Randy.NET</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/Verrok/Randy.NET/blob/master/LICENSE</PackageLicenseUrl>
        <PackageIconUrl>https://i.ibb.co/WKH3n2r/randy.png</PackageIconUrl>
        <RepositoryUrl>https://github.com/Verrok/Randy.NET</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2.1.0</PackageVersion>
        <PackageReleaseNotes>Added different int bases support</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AutoMapper" Version="9.0.0" />
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

</Project>
